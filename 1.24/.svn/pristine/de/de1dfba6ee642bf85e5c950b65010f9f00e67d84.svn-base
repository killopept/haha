.page-whiteListManage {
  background-color: #fff;
  border: 1px solid #e2e8ec;
  border-bottom: none;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
  position: relative;
  min-height: 100%;
  * {
    box-sizing: border-box;
  }
  .el-form-item__label {
    &:before {
      display: inline-block;
      margin-right: 4px;
      content: "*";
      font-family: SimSun;
      line-height: 1;
      font-size: 12px;
      color: #f04134;
    }
    &:after {
      content: ":";
      margin: 0 8px 0 2px;
      position: relative;
      top: -.5px;
    }
  }
}