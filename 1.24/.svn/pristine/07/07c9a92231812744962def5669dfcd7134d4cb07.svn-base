.pageCheckList{
  .page-table{
    width:100%;
    margin:0 auto;
    .el-table{
      border-radius:5px;
      font-size:12px;     
      tbody{
        tr{
          td{
            height:43px;
            padding:0;
            color: rgba(0,0,0,.65);
          }
        }
      } 
      .el-button{
        &+.el-button{
          margin-left: 0px;
        }
      }
    }
    .tradeDetails{
      padding:0 16px;
      tr{
        td{
          border:none;
          height:30px;
          padding:0;
          font-size: 12px;
          color:#000;
          text-align:right;
          &+td{
            color:rgba(0,0,0,.65);
            text-align:left;
          }
        }
      }
    }
    .agreeDetails{
      tr{
        td{
          border:none;
          min-width:142px;
          height:40px;
          padding:0;
          font-size: 12px;
          color:#000;
          text-align:right;
          &+td{
            color:rgba(0,0,0,.65);
            text-align:left;
          }
        }
      }
    }
    .rejectDetails{
      tr{
        td{
          border:none;
          width:90px;
          height:40px;
          padding:0;
          font-size: 12px;
          color:#000;
          text-align:right;
          .el-form-item__label{
            font-size: 12px;
            color:#000;   
          }
          &+td{
            color:rgba(0,0,0,.65);
            text-align:left;
            width:330px;
          }
        }
      }
    }
    .el-dialog__wrapper{
      .el-dialog{
        width:520px;
      }
    }
  }
}